doctype html
html(lang="en")
head
  include head.jade
body
  include nav.jade

  div.container
    div.header
      h3.text-muted
      
    div.jumbotron
      h1 Bluemix Content Microservice
      p.lead Upload new content assets to be discovered in the Bluemix Community and product pages.
      p.clearfix 
      
      div#error
      
      form(method="post",onsubmit="return checkLogin();")
        div.form-group
          label(for="password") Password
          input.form-control(id="password",type="password",name="password")
        button.btn.btn-default(type="submit") Submit


